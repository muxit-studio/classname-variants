{
  "name": "classname-variants",
  "version": "1.3.2",
  "description": "Variant API for plain class names, with solidjs support",
  "author": "Felix Gnass <fgnass@gmail.com>",
  "license": "MIT",
  "repository": "muxit-studio/classname-variants",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "type": "./lib/index.d.ts"
    },
    "./solid": {
      "import": "./lib/solid.js",
      "type": "./lib/solid.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/index.d.ts"
      ],
      "react": [
        "./lib/react.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "vite serve dev",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "tailwind",
    "css",
    "classname",
    "variants",
    "solidjs"
  ],
  "dependencies": {
    "solid-js": "^1.7.5"
  },
  "devDependencies": {
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vite-plugin-solid": "^2.7.0"
  }
}
